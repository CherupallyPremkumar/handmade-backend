<?xml version="1.0" encoding="UTF-8"?>
<states>
    <flow id="workflowTaskFlow" default="true">
        <manual-state id="STARTED" initialState="true">
            <on eventId="complete" newStateId="COMPLETED" componentName="workflowTaskCompleteAction"/>
            <on eventId="fail" newStateId="FAILED" componentName="workflowTaskFailAction"/>
        </manual-state>

        <manual-state id="COMPLETED" meta-endState="true"/>
        <manual-state id="FAILED" meta-endState="true"/>
    </flow>
</states>
