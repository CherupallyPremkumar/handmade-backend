{
	"name": "_tenantStateEntityService_",
	"id": "tenantService",
	"healthCheckerName":"tenantHealthChecker",
	"operations": [
		{
			"name": "retrieve",
			"url": "/tenant/{id}",
			"httpMethod": "GET",
			"produces": "JSON",
			"consumes": "JSON",
			"input": "",
			"output": "org.chenile.workflow.dto.StateEntityServiceResponse",
			"params": [
				{
					"name": "id",
					"type": "HEADER",
					"paramClass": "java.lang.String",
					"description": "ID of the tenant to retrieve"
				}
			]
		},
		{
			"name": "create",
			"url": "/tenant",
			"httpMethod": "POST",
			"produces": "JSON",
			"consumes": "JSON",
			"input": "com.netscoretech.pos.tenant.model.Tenant",
			"output": "org.chenile.workflow.dto.StateEntityServiceResponse",
			"params": [
				{
					"name": "entity",
					"type": "BODY",
					"description": "The tenant that needs to be created",
					"paramClass": "org.chenile.stm.StateEntity"
				}
			]
		},
		{
			"name": "processById",
			"url": "/tenant/{id}/{eventID}",
			"httpMethod": "PUT",
			"produces": "JSON",
			"consumes": "JSON",
			"input": "com.netscoretech.pos.tenant.model.Tenant",
			"output": "org.chenile.workflow.dto.StateEntityServiceResponse",
			"bodyTypeSelectorComponentNames": ["tenantBodyTypeSelector","subclassBodyTypeSelector"],
			"params": [
				{
					"name": "id",
					"type": "HEADER",
					"description": "The ID of the tenant that needs to receive this event",
					"paramClass": "java.lang.String"
				},
				{
					"name": "eventID",
					"type": "HEADER",
					"description": "The event ID that needs to be sent to the tenant",
					"paramClass": "java.lang.String"
				},
				{
					"name": "payload",
					"type": "BODY",
					"description": "The payload for the event",
					"paramClass": "java.lang.Object"
				}
			]
		}
	]
}
